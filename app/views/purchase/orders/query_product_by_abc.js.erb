$(".fixed_panel").html("");
<% @result_products.each do |product| %>
    if($("#<%= product.mark %>").size()==1){
        str = '<div onclick="choose_this('+'\'<%= product.chinese_name %>\''+')" style="cursor:hand;width: 100%;height: 15px; "  >';
        str += '<h6><a href="javascript:void(0)"><%= product.chinese_name %></a></h6>';
        str += '</div>';
        str += '<div style="height: 2px"></div>';
        $("#<%= product.mark %>").append(str);
    }else{
        str = '<div onclick="choose_this('+'\'<%= product.chinese_name %>\''+')" style="cursor:hand;width: 100%;height: 15px; "  >';
        str += '   <h6><a href="javascript:void(0)"><%= product.chinese_name %></a></h6>';
        str += '</div>';
        str += '<div style="height: 2px"></div>';
        $("#others").append(str);
    }

<% end %>