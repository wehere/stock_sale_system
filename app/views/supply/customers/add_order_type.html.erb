<br/>
<%= form_tag '/supply/customers/add_order_type', method: :post do %>
    供应商名称
    <%= @supplier.simple_name %>
    <%= hidden_field_tag :supplier_id, @supplier.id %>
    客户名称：
    <%= @customer.simple_name %>
    <%= hidden_field_tag :customer_id, @customer.id %>
    单据类型
    <%= text_field_tag :name, params[:name], class: 'sx-display-inline sx-width-auto'  %>
    <%= submit_tag '保存', class: "button primary" %>
<% end %>